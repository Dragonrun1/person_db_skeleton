{
    "name": "dragonrun1/person_db_skeleton",
    "description": "Skeleton for common person database tables in MySQL and Doctrine.",
    "license": "BSD-3-Clause",
    "version": "0.0.2",
    "authors": [
        {
            "name": "Michael Cummings", "email": "dragonrun1@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PersonDBSkeleton\\": [
                "bin/", "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Spec\\": "tests/specs/"
        }
    },
    "config": {
        "optimize-autoloader": true, "sort-packages": true
    },
    "require": {
        "php": ">=7.3,<7.5",
        "ext-PDO": ">=1.0.0",
        "ext-SPL": "*",
        "ext-json": "*",
        "ext-pdo_mysql": "*",
        "ext-sodium": "*",
        "doctrine/migrations": "^2.2",
        "doctrine/orm": "^2.7",
        "dragonrun1/uuid64type": "@dev",
        "jdorn/sql-formatter": "^1.2",
        "josegonzalez/dotenv": "^3.2"
    },
    "require-dev": {
        "friends-of-phpspec/phpspec-code-coverage": "^4.3",
        "phpspec/nyan-formatters": "dev-master",
        "phpspec/phpspec": "^5.1.0",
        "roave/security-advisories": "dev-master"
    }
}
